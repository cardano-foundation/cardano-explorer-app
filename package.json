{
  "name": "cardano-explorer-app",
  "version": "0.1.0",
  "description": "Cardano Explorer App",
  "author": "Daedalus Team @ Input Output HK",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next dev source -p 4000",
    "build": "next build source",
    "test": "jest",
    "e2e": "cucumber-js --require-module ts-node/register --require 'features/steps/**/*.ts' -f node_modules/cucumber-pretty --format-options '{\"snippetInterface\": \"async-await\"}'",
    "e2e:watch": "nodemon --watch source --watch features -e js,ts,feature --exec yarn test --tags @watch",
    "generate:graphql-typings": "SCHEMA_URI=${SCHEMA_URI:-http://localhost:3100} graphql-codegen",
    "generate:graphql-schema-reference": "get-graphql-schema ${SCHEMA_URI:-http://localhost:3100} > schema.graphql",
    "export": "next export source --outdir build/static",
    "static:build": "NODE_ENV=production yarn build && yarn export",
    "static:serve": "serve -p 4001",
    "static:preview": "yarn static:build && yarn static:serve",
    "analyze": "BUNDLE_ANALYZE=browser yarn build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prettier": "./node_modules/.bin/prettier \"**/*.*\"",
    "prettier:check": "yarn prettier --check",
    "prettier:format": "yarn prettier --write --loglevel warn",
    "prettier:fix-staged": "pretty-quick --staged",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o build/storybook",
    "start-dependencies": "docker-compose up",
    "stop-dependencies": "docker-compose down",
    "deploy:byron:staging": "node deploy/byron_staging",
    "deploy:byron:incentivized-testnet": "node deploy/byron_incentivized_testnet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix-staged",
      "pre-push": "yarn prettier:check && yarn lint && yarn test && yarn build"
    }
  },
  "dependencies": {
    "@svgr/webpack": "4.3.2",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.11",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.15",
