{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "array-type": false,
    "interface-over-type-literal": false,
    "import-name": false,
    "quotemark": [true, "single", "jsx-double"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "no-var-requires": false
  }
}
